{
  "name": "resty",
  "version": "1.0.2",
  "description": "Syntactic sugar for RESTful APIs",
  "main": "dist/resty.js",
  "module": "dist/resty.mjs",
  "typings": "dist/resty.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "postinstall": "is-ci || husky install",
    "postpublish": "pinst --enable",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "test": "jest"
  },
  "keywords": [
    "api",
    "app",
    "framework",
    "http",
    "rest",
    "restful",
    "router",
    "web"
  ],
  "license": "MIT",
  "author": "Gabriel Vaquer <brielov@icloud.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brielov/resty.git"
  },
  "bugs": {
    "url": "https://github.com/brielov/resty/issues"
  },
  "homepage": "https://github.com/brielov/resty#readme",
  "dependencies": {
    "bytes": "^3.1.0",
    "http-status-codes": "^2.1.4",
    "typed": "^1.1.3"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "esbuild": "^0.13.11",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.6",
    "mock-http": "^1.1.0",
    "pinst": "^2.1.6",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.3",
    "rollup-plugin-dts": "^4.0.0",
    "rollup-plugin-esbuild": "^4.6.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  }
}
